{
  "name": "binsarjr.com",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bun run vite dev",
    "build": "bun run prebuild && bun run vite build",
    "build:fast": "bun run vite build",
    "prebuild": "bun run scripts/prebuild.js",
    "preview": "bun run vite preview",
    "prepare": "bun run svelte-kit sync || echo ''",
    "check": "bun run svelte-kit sync && bun run svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun run svelte-kit sync && bun run svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "bun run prettier --write .",
    "lint": "bun run prettier --check . && bun run eslint .",
    "clean": "rm -rf .svelte-kit build node_modules/.vite",
    "reinstall": "rm -rf node_modules bun.lock && bun install",
    "dev:host": "bun run vite dev --host",
    "build:analyze": "bun run vite build --mode analyze",
    "config:update": "bun scripts/update-personal-config.js"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "mdsvex": "^0.12.3",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-toc": "^9.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^6.2.6"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.0.0",
    "aos": "^2.3.4",
    "date-fns": "^4.1.0",
    "lucide-svelte": "^0.515.0"
  }
}